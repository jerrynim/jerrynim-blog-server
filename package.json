{
  "name": "jerrynim-blog-server",
  "version": "1.0.0",
  "description": "server for blog",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.493.0",
    "graphql-tools": "^4.0.5",
    "graphql-yoga": "^1.18.1",
    "merge-graphql-schemas": "^1.5.8",
    "prisma": "^1.34.1",
    "prisma-client-lib": "^1.34.1"
  },
  "devDependencies": {
    "babel-runtime": "^6.26.0",
    "copy": "^0.3.2",
    "gql-merge": "^0.0.6",
    "gulp": "^4.0.2",
    "gulp-typescript": "^5.0.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "prisma": "prisma deploy && prisma generate",
    "pretypes": "gql-merge --out-file ./schema.graphql ./src/Api/**/**.graphql ",
